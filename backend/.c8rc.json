{
  "all": true,
  "include": ["src/**/*.ts"],
  "exclude": [
    "src/**/*.test.ts",
    "src/tests/**",
    "**/*.d.ts",
    "src/index.ts"
  ],
  "reporter": ["text", "json", "html", "lcov", "json-summary"],
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 80,
  "statements": 80,
  "per-file": false,
  "skip-full": false,
  "clean": true
}
